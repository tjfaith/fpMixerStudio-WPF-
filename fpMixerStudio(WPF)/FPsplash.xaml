<Window x:Class="fpMixerStudio_WPF_.FPsplash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FPsplash"  Height="300" Width="300" WindowStartupLocation="CenterScreen" Icon="resources/fplogo4.png" AllowsTransparency="True" WindowStyle="None" Loaded="Window_Loaded_1">
    
    <Window.Background>
        <ImageBrush ImageSource="resources/fplogo1.png" />
    </Window.Background>
    <Grid Loaded="Grid_Loaded_1">
        
        <Image Source="resources/fplogo6.png" Margin="0,0,0,10"/>
        <Image Source="resources/fplogo27.png" Margin="10,0,236,10"/>
         <Image Source="resources/fplogo27.png" Margin="236,0,10,10"/>
        <Image Source="resources/fplogo4.png" Margin="82,24,73,50"/>
        <Image Source="resources/fplogo5.png" Margin="64,250,64,10"/>
        <TextBlock  x:Name="LoadingTime"        HorizontalAlignment="Left" Margin="120,49,0,0" TextWrapping="Wrap"  Foreground="White" Text="Loading......."   VerticalAlignment="Top"/>

    </Grid>
    
